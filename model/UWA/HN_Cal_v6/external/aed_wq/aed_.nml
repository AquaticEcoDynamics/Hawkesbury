&aed_pathogens
   num_pathogens = 2
   the_pathogens = 2 ,3  !,5
   dbase = '../../external/aed2/aed2_pathogen_pars.nml'
   resuspension = 0
   num_ss = 1
   ss_set = 0
   ss_tau = 0.1
   ss_ke = 0.1
   sim_sedorgs = .false.
   oxy_variable = ''
   epsilon = 0
   tau_0 = 0.1
   tau_0_min = 0.1
   Ktau_0 = 0.1
   extra_diag = .true.
   diag_level = 10
   !att_ts =
/

